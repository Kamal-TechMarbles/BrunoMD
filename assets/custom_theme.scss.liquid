$screen-min-mac: 1920px;
$screen-msd-max: 1024px;
$screen-xl-max: 1680px;
$screen-lg-max: 1400px;
$screen-md-max: 1280px;
$screen-medium: 768px;
$screen-sm-max: 750px;
$screen-xs-max: 480px;
$screen-medium: 768px;
$screen-ipad: 800px;
$mobile: 749px;

@mixin mac {
    @media ( min-width: #{$screen-min-mac} ) {
        @content;
    }
}
@mixin xl {
    @media ( max-width: #{$screen-xl-max} ) {
        @content;
    }
}
@mixin lg {
    @media ( max-width: #{$screen-lg-max} ) {
        @content;
   }
}
@mixin md {
    @media ( max-width: #{$screen-md-max} ) {
        @content;
    }
}
@mixin msd {
    @media ( max-width: #{$screen-msd-max} ) {
        @content;
    }
}
@mixin sm {
    @media ( max-width: #{$screen-sm-max} ) {
        @content;
    }
}
@mixin xs {
    @media ( max-width: #{$screen-xs-max} ) {
        @content;
    }
}
@mixin medium {
    @media ( max-width: #{$screen-medium} ) {
        @content;
    }
}
@mixin ipadscreen {
    @media ( max-width: #{$screen-ipad} ) {
        @content;
    }
}
@mixin mobile {
    @media ( max-width: #{$mobile} ) {
        @content;
    }
}

