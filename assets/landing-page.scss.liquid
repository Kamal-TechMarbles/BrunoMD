.brunoMD-content {
    border-bottom: 2px solid #ececee;
    padding: 40px 60px 80px;
    text-align: center;
    margin: 30px auto 0;
    height: 100%;
    width: 100%;
    bottom: 0;
    right: 0;
    left: 0;
    top: 0;
    @include xl {
        padding: 60px;
    }
    @include xs {
        padding: 25px 20px;
        max-width: 320px;
    }
    p {
        margin: 0 auto 10px;
        color: #636464;
    }
    p.pro-price {
        margin-bottom: 30px;
        text-align: left;
        max-width: 420px;
        @include xs {
            margin-bottom: 20px;
            font-size: 14px;
        }
        span.price-value {
            font-family: roboto;
            font-size: 32px;
            @include xs {
                font-size: 18px;
            }
        }
        span.yousave-span {
            padding-left: 5px;
            color: #d73948;
            font-size: 15px;
        }
    }
    .quantity {
        display: inline-block;
        width: 100%;
        label {
            text-align: center;
            cursor: pointer;
            width: 150px;
            float: left;
            input {
                display: none;
            }
            .pack {
                border: 2px solid #e7e7e7;
                margin: 0 auto 10px;
                position: relative;
                border-radius: 6px;
                text-align: center;
                overflow: hidden;
                height: 150px;
                padding: 5px;
                img {
                    position: absolute;
                    max-width: 90px;
                    opacity: 0.3;
                    margin: auto;
                    bottom: 0;
                    right: 0;
                    left: 0;
                    top: 0;
                }
            }
            input[type="radio"]:checked + .pack {
                border-color: #3590d0;
                background: #eaf5fe;
            }
            input[type="radio"]:checked + .pack img {
                opacity: 1;
            }
        }
        label + label {
            margin-left: 18px;
        }
    }
    .quantity-subs {
        display: inline-block;
        max-width: 420px;
        margin: 0 auto;
        width: 100%;
        @include xs {
            max-width: 280px;
        }
        label {
            text-align: center;
            cursor: pointer;
            width: 210px;
            float: left;
            @include xs {
                max-width: 140px;
            }
            input {
                display: none;
            }
            .pack {
                border-radius: 10px 0 0 10px;
                border: 1px solid #d8d8d8;
                border-right: 1px solid #fff;
                padding: 24px 15px 15px;
                position: relative;
                text-align: center;
                height: 100px;
                @include xs {
                    padding: 15px;
                }
                span {
                    font-size: 130%;
                }
            }
            input[type="radio"]:checked + .pack {
                border: 1px solid #00a3dc !important;
                background: #eaf1f1;
            }
            input[type="radio"]:checked + .pack:before {
                background-image: url(https://cdn.shopify.com/s/files/1/0053/9952/t/518/assets/checkmark.svg);
                background-repeat: no-repeat;
                background-size: 100% auto;
                position: absolute;
                margin: 0 auto;
                content: '';
                height: 16px;
                width: 16px;
                top: -9px;
                right: 0;
                left: 0;
            }
        }
        label + label {
            .pack {
                border-radius: 0 10px 10px 0;
                border: 1px solid #d8d8d8;
                border-left: 1px solid #fff;
            }
        }
    }
    .quantity-item {
        label {
            font-family: Gotham-Medium;
            border: 1px solid #d8d8d8;
            text-transform: uppercase;
            display: inline-block;
            border-radius: 10px;
            padding: 12px 15px;
            position: relative;
            margin: 10px auto;
            background: #fff;
            max-width: 420px;
            cursor: pointer;
            color: #000;
            width: 100%;
            @include xs {
                padding: 10px 12px;
                margin: 5px auto;
                text-align: left;
                font-size: 13px;
            }
            select {
                padding: 2px 0 2px 15px;
                background: #fff;
                max-width: 300px;
                border: none;
                float: right;
                width: 100%;
                @include xs {
                    text-align: left;
                    max-width: none;
                    font-size: 13px;
                    padding: 2px 0;
                    text-indent: 0;
                    display: block;
                    float: none;
                    width: 100%;
                }
                option {
                    width: 100%;
                }
            }
            &:after {
                border-right: 5px solid transparent;
                border-left: 5px solid transparent;
                border-top: 5px solid #00a2df;
                position: absolute;
                content: '';
                right: 15px;
                top: 21px;
                height: 0;
                width: 0;
                @include xs {
                    right: 10px;
                    top: 28px;
                }
            }
        }
    }
    .buynow-btns {
        text-align: center;
        max-width: 420px;
        margin: 0 auto;
        .buy-now {
            background: #300370;
            padding: 12px 20px 10px;
            width: 320px;
            margin: 10px auto 0;
            position: relative;
            z-index: 9;
