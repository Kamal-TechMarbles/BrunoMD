.rcp-header {
    -webkit-transition: all 0.3s ease-in-out 0s;
    -moz-transition: all 0.3s ease-in-out 0s;
    -ms-transition: all 0.3s ease-in-out 0s;
    -o-transition: all 0.3s ease-in-out 0s;
    transition: all 0.3s ease-in-out 0s;
    border-bottom: 2px solid #ececee;
    position: relative;
    background: #fff;
    padding: 15px 0;
    z-index: 9999;
    width: 100%;
    right: 0;
    left: 0;
    top: 0;
    .rcp-logo {
        vertical-align: middle;
        display: inline-block;
        font-size: 0;
        width: 220px;
    }
    .rcp-menus {
        width: calc(100% - 230px);
        vertical-align: middle;
        display: inline-block;
        padding-left: 12px;
        text-align: right;
        .menu-item {
            vertical-align: middle;
            display: inline-block;
            position: relative;
            padding: 0 15px;
            @include md {
                font-size: 14px;
                padding: 0 5px;
            }
            &:after {
                -webkit-transition: all 0.3s ease-in-out 0s;
                -moz-transition: all 0.3s ease-in-out 0s;
                transition: all 0.3s ease-in-out 0s;
                border-bottom: 2px solid #300370;
                position: absolute;
                display: block;
                bottom: -34px;
                content: '';
                height: 1px;
                width: 0;
                right: 0;
                left: 0;
            }
            &:hover:after {
                width: 100%;
            }
            &:last-child {
                @include sm {
                    display: none
                }
            }
        }
    }
}
