$screen-pc-mac: 1920px;
$screen-xl-min: 1680px;
$screen-lg-max: 1400px;
$screen-md-max: 1280px;
$screen-bd-max: 1170px;
$screen-sm-max: 1024px;
$screen-cd-max: 880px;
$screen-mobile: 768px;
$screen-xs-max: 749px;
@mixin mac {
    @media ( min-width: #{$screen-pc-mac} ) {
        @content;
    }
}
@mixin xl {
    @media ( min-width: #{$screen-xl-min} ) {
        @content;
    }
}
@mixin lg {
    @media ( max-width: #{$screen-lg-max} ) {
        @content;
   }
}
@mixin md {
    @media ( max-width: #{$screen-md-max} ) {
        @content;
    }
}
@mixin bd {
    @media ( max-width: #{$screen-bd-max} ) {
        @content;
    }
}
@mixin cd {
    @media ( max-width: #{$screen-cd-max} ) {
        @content;
    }
}
@mixin sm {
    @media ( max-width: #{$screen-sm-max} ) {
        @content;
    }
}
@mixin xs {
    @media ( max-width: #{$screen-xs-max} ) {
        @content;
    }
}
@mixin mobile {
    @media ( max-width: #{$screen-mobile} ) {
        @content;
    }
}

