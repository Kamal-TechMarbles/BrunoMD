            font-size: 22px;
            text-transform: capitalize;
            border: none;
            text-align: center;
            letter-spacing: 0;
            @include xs {
                padding: 12px 20px 10px;
                width: 200px;
                font-size: 18px;
            }
        }
    }
    .subs-statement {
        margin: 40px auto 0;
        @include xs {
            margin: 25px auto 0;
        }
