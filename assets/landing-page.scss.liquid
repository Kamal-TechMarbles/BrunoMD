/* Lazy Load QuickFix */
.lazyload, .lazyloaded{
    transition: 400ms linear; 
}

.lazyload {
  background-color: white;
  position: relative;
  overflow: hidden;
  border-radius: 30px !important;
  height: 30px !important;
  width: 30px !important;
  display: inline-block;
  text-align: center;
  padding: 0 !important;
  box-shadow: 0 0 20px -9px black;

  &::before{
    position: absolute;
    content: '';
    height: 30px;
    width: 30px;
    border-radius: 30px;
    background-color: #ffffff;
    left: 0;
    top: 0;
    z-index: 1;
  }

  &::after {
    position: absolute;
    content: '';
    height: 20px;
    width: 20px;
    border-radius: 20px;
    background-color: #ffffff;
    border: dashed 1px #54337a;
    left: 5px;
    top: 5px;
    z-index: 1;

    animation: spin 2s linear infinite;  
  }
}
@keyframes spin { 100% { -webkit-transform: rotate(360deg); transform:rotate(360deg); } }