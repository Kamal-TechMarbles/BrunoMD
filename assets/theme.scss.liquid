  margin-top: 0px;
}

.product-form__controls-group ~ .product-form__controls-group--submit {
  margin-top: 15px;
}

.product-form__item {
  flex-grow: 0;
  flex-basis: 100%;
  margin-bottom: 10px;
  padding: 0;

  @include media-query($large-up) {
    flex-basis: 50%;
    padding: 0 5px;

